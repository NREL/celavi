********************************************************
	.product choice
********************************************************~
	|

changing fraction reuse=
	IF THEN ELSE(check fraction sum < 1 - rate of increasing reuse fraction :AND:
	             Fraction Reuse < 1 - rate of increasing reuse fraction,
	             increase reuse * rate of increasing reuse fraction / TIME STEP,
	             0)
	~	Dmnl/year
	~		|

check fraction sum=
	Fraction Reuse + Fraction Remanufacture + Fraction Recycle
	~	Dmnl [0,1]
	~		|

Fraction Recycle= INTEG (
	increasing fraction recycle,
		fraction used product recycled initial value)
	~	Dmnl [0,1]
	~		|

Fraction Remanufacture= INTEG (
	increasing fraction remanufacture,
		fraction used product remanufactured initial value)
	~	Dmnl [0,1]
	~		|

Fraction Reuse= INTEG (
	changing fraction reuse,
		fraction used product reused initial value)
	~	Dmnl [0,1]
	~		|

rate of increasing reuse fraction= WITH LOOKUP (
	reuse favorability,
		([(0,0)-(10,0.01)],(0,0.0075),(0.275229,0.006),(0.519878,0.005),(0.99,0.004),(1,0),\
		(2,0),(10,0) ))
	~	Dmnl
	~	\!reuse favorability\!rate of increasing fraction reuse
	|

recycle favorability=
	IF THEN ELSE(lowest cost pathway = recycle favorability over linear,
	             recycle favorability over linear,
	             10)
	~	Dmnl [0,?]
	~		|

recycle favorability over linear=
	recycled material cost / linear material cost
	~	Dmnl [0,?]
	~		|

increase recycle=
	IF THEN ELSE(recycle favorability over linear < 1,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

increase remanufacture=
	IF THEN ELSE(remanufacture favorability over linear < 1,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

increase reuse=
	IF THEN ELSE(reuse favorability over linear < 1,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

increasing fraction recycle=
	IF THEN ELSE(check fraction sum < 1 - rate of increasing recycle fraction :AND:
	             Fraction Recycle < 1 - rate of increasing recycle fraction,
	             increase recycle * rate of increasing recycle fraction / TIME STEP,
	             0)
	~	Dmnl/year
	~		|

increasing fraction remanufacture=
	IF THEN ELSE(check fraction sum < 1 - rate of increasing remanufacture fraction :AND:
	             Fraction Remanufacture < 1 - rate of increasing remanufacture fraction,
	             increase remanufacture * rate of increasing remanufacture fraction / TIME STEP\
		,
	             0)
	~	Dmnl/year
	~		|

lowest cost pathway=
	MINLIST(reuse favorability over linear, remanufacture favorability over linear, recycle favorability over linear\
		, 1000)
	~	Dmnl [0,?]
	~		|

rate of increasing recycle fraction= WITH LOOKUP (
	recycle favorability,
		([(0,0)-(10,0.01)],(0,0.01),(0.275229,0.00964912),(0.422018,0.00995614),(1,0.0090825\
		),(1.40673,0.00929825),(2,0),(10,0
		) ))
	~	Dmnl
	~	\!recycle favorability
	|

rate of increasing remanufacture fraction= WITH LOOKUP (
	remanufacture favorability,
		([(0,0)-(10,0.008)],(0,0.00736842),(0.165138,0.00526316),(0.360856,0.00342105),(0.550459\
		,0.00184211),(1,0),(2,0),(10,0
		) ))
	~	Dmnl
	~	\!remanufacture favorability
	|

reuse favorability=
	IF THEN ELSE(lowest cost pathway = reuse favorability over linear,
	             reuse favorability over linear,
	             10)
	~	Dmnl [0,?]
	~		|

reuse favorability over linear=
	reused material cost / linear material cost
	~	Dmnl [0,?]
	~		|

remanufacture favorability over linear=
	remanufactured material cost / linear material cost
	~	Dmnl [0,?]
	~		|

remanufacture favorability=
	IF THEN ELSE(lowest cost pathway = remanufacture favorability over linear,
	             remanufacture favorability over linear,
	             10)
	~	Dmnl [0,?]
	~		|

fraction used product reused initial value=
	0
	~	Dmnl [0,1,0.01]
	~		|

fraction used product recycled initial value=
	0.3
	~	Dmnl [0,1,0.01]
	~	Starting value is an estimate of early-80s steel recycling rates, \
		approximated from a 1991 steel production statistic from the USGS Iron & \
		Steel Mineral Commodity Summary from 1996. Source link: \
		https://s3-us-west-2.amazonaws.com/prd-wret/assets/palladium/production/min\
		eral-pubs/iron-steel/isteemcs96.pdf
	|

fraction used product remanufactured initial value=
	0
	~	Dmnl [0,1,0.01]
	~		|

********************************************************
	.material flows
********************************************************~
	|

disposing=
	reaching end of life - reusing - remanufacturing - recycling - reusing in other sectors
	~	metric ton/year [0,?]
	~		|

annual demand=
	material use per year
	~	metric ton/year [0,?,10000]
	~		|

aggregating recycled materials=
	recycling + recycling failed remanufactured - landfilling nonrecyclables
	~	metric ton/year [0,?]
	~		|

aggregating remanufactured products=
	remanufacturing 
	+ remanufacturing nonreusables
	- landfilling failed remanufactured 
	- recycling failed remanufactured
	~	metric ton/year [0,?]
	~		|

aggregating reused products=
	reusing - remanufacturing nonreusables
	~	metric ton/year [0,?]
	~		|

annual virgin material demand=
	MAX(0,
	    annual demand - aggregating recycled materials - aggregating reused products - aggregating remanufactured products\
		)
	~	metric ton/year [0,?]
	~		|

Product Reuse= INTEG (
	reusing-aggregating reused products-remanufacturing nonreusables,
		0)
	~	metric ton [-10,10]
	~		|

distributing=
	extracting + aggregating recycled materials
	~	metric ton/year [0,?]
	~		|

extracting=
	annual virgin material demand
	~	metric ton/year [0,?]
	~		|

Products Sent to Other Sectors= INTEG (
	reusing in other sectors,
		0)
	~	metric ton [0,?]
	~		|

Raw Material Extraction= INTEG (
	-extracting,
		1e+15)
	~	metric ton [0,?]
	~		|

reaching end of life= DELAY MATERIAL (
	shipping, component lifetime , initial components in use / component lifetime , 0)
	~	metric ton/year [0,?]
	~	* used to be a DELAY CONVEYOR( shipping ,
		                product lifetime , 
		                0 ,
		                initprofile , 
		                initial products at end of life , 
		                product lifetime )
		* first argument to DELAY CONVEYOR was originally transporting * IF THEN \
		ELSE(Products in Use <= 0, 0, 1)
	|

recycling=
	INTEGER(reaching end of life * Fraction Recycle)
	~	metric ton/year [0,?]
	~		|

fraction used product disposed=
	1 - Fraction Reuse - fraction used product reused in other sectors - Fraction Remanufacture\
		 - Fraction Recycle
	~	Dmnl [0,1]
	~	default value 0.67
	|

remanufacturing=
	reaching end of life * Fraction Remanufacture
	~	metric ton/year [0,?]
	~		|

remanufacturing nonreusables=
	reusing * fraction reused product remanufactured
	~	metric ton/year [0,?]
	~		|

Landfill and Incineration= INTEG (
	landfilling+landfilling failed remanufactured+landfilling nonrecyclables,
		0)
	~	metric ton [0,?]
	~		|

Landfill Transport= INTEG (
	disposing-landfilling,
		0)
	~	metric ton [0,?]
	~		|

landfilling=
	disposing
	~	metric ton/year [0,?]
	~		|

landfilling failed remanufactured=
	(remanufacturing + remanufacturing nonreusables) * fraction of remanufacture to landfill
	~	metric ton/year [0,?]
	~		|

landfilling nonrecyclables=
	(recycling + recycling failed remanufactured) * fraction of recycling to landfill
	~	metric ton/year [0,?]
	~		|

Material Distribution= INTEG (
	aggregating recycled materials+extracting-distributing,
		100)
	~	metric ton [0,?]
	~		|

Material Recycle= INTEG (
	recycling
	+ recycling failed remanufactured
	- aggregating recycled materials
	- landfilling nonrecyclables,
		0)
	~	metric ton [-0.1,?]
	~	aka recycling
	|

producing=
	distributing + aggregating remanufactured products
	~	metric ton/year [0,?]
	~		|

Product Distribution= INTEG (
	aggregating reused products
	+ producing
	- shipping,
		100)
	~	metric ton [0,?]
	~		|

Product Remanufacture= INTEG (
	remanufacturing
	+ remanufacturing nonreusables
	- aggregating remanufactured products
	- landfilling failed remanufactured
	- recycling failed remanufactured,
		0)
	~	metric ton [-10,10]
	~		|

tallying extraction=
	extracting
	~	metric ton/year [0,?]
	~		|

Production= INTEG (
	aggregating remanufactured products + distributing - producing,
		100)
	~	metric ton [0,?]
	~		|

Products at End of Life= INTEG (
	reaching end of life-disposing-recycling-remanufacturing-reusing-reusing in other sectors\
		,
		0)
	~	metric ton [-1,?]
	~		|

Products in Use= INTEG (
	shipping-reaching end of life,
		initial components in use)
	~	metric ton [0,?]
	~		|

Total Extraction= INTEG (
	tallying extraction,
		0)
	~	metric ton [0,?]
	~		|

reusing in other sectors=
	reaching end of life * fraction used product reused in other sectors
	~	metric ton/year [0,?]
	~		|

recycling failed remanufactured=
	(remanufacturing + remanufacturing nonreusables) * fraction remanufactured product to recycle
	~	metric ton/year [0,?]
	~	components that could not be put back into a using product
	|

reusing=
	INTEGER( reaching end of life * Fraction Reuse)
	~	metric ton/year [0,?]
	~		|

shipping= DELAY MATERIAL (
	producing + aggregating reused products, 
	supply chain delay,
	0,
	0)
	~	metric ton/year [0,?]
	~		|

fraction used product reused in other sectors=
	0
	~	Dmnl [0,1,0.01]
	~		|

supply chain delay=
	0.5
	~	year [0,3,0.25]
	~	default value 0.5
	|

initial components in use=
	4000
	~	metric ton [0,?]
	~	We assume that no utility-scale wind turbines are installed before the \
		model begins.
	|

fraction of recycling to landfill=
	0.05
	~	Dmnl [0,1,0.01]
	~	default value 0.01
	|

fraction of remanufacture to landfill=
	0.05
	~	Dmnl [0,1,0.01]
	~	default value 0.01
	|

fraction remanufactured product to recycle=
	0.01
	~	Dmnl [0,1,0.01]
	~		|

fraction reused product remanufactured=
	0.01
	~	Dmnl
	~		|

component lifetime=
	20
	~	year [0,50,0.5]
	~	Default value 20 or 25, depending on assumptions.
	|

********************************************************
	.metrics
********************************************************~
	|

externalities total=
	IF THEN ELSE(annual demand = 0,
	             0,
	             ( externalities from reusing
	               + externalities from remanufacturing
	               + externalities from recycling
	               + externalities from extracting
	               + externalities from total transportation ) / annual demand
	             )
	~	impact/metric ton [0,?]
	~		|

externalities from total transportation=
	externalities from recycling transportation+externalities from landfill transportation\
		+externalities from remanufacturing transportation+externalities from reuse transportation
	~	impact/year [0,?]
	~		|

externalities from extracting=
	extracting * externality factor of extracting process
	~	impact/year [0,?]
	~		|

externalities from recycling=
	aggregating recycled materials * externality factor of recycling process
	~	impact/year [0,?]
	~		|

externalities from remanufacturing=
	aggregating remanufactured products * externality factor of remanufacturing process
	~	impact/year
	~		|

externalities from reusing=
	aggregating reused products * externality factor of reusing process
	~	impact/year [0,?]
	~		|

cumulative landfill fraction=
	Landfill and Incineration / (Total Extraction + 0.001)
	~	Dmnl [0,?]
	~		|

externalities from landfill transportation=
	externality factor of transportation * landfilling * miles from end use location to landfill
	~	impact/year
	~		|

externalities from recycling transportation=
	externality factor of transportation *
	( recycling * miles from end use location to recycling facility
	  + recycling failed remanufactured * miles from remanufacturing facility to recycling facility
	  + landfilling nonrecyclables * miles from recycling facility to landfill
	  + aggregating recycled materials * miles from recycling to distribution facility )
	~	impact/year
	~		|

externalities from remanufacturing transportation=
	externality factor of transportation *
	( remanufacturing * miles from end use location to remanufacturing facility 
	  + remanufacturing nonreusables * miles from reuse facility to remanufacturing facility
	  + aggregating remanufactured products * miles from remanufacturing facility to product distribution facility\
		 
	  + landfilling failed remanufactured * miles from remanufacturing facility to landfill\
		 )
	~	impact/year
	~		|

externalities from reuse transportation=
	externality factor of transportation * 
