********************************************************~
	|

one MW=
	1
	~	MW [1,1,1]
	~		|

one Year=
	1
	~	year [1,1]
	~		|

one Unit=
	1
	~	each [1,1]
	~		|

********************************************************
	.limitations
********************************************************~
	|

tallying reuse=
	aggregating reused products
	~	metric ton/year [0,?]
	~		|

Cumulative Recycle= INTEG (
	tallying recycle,
		0)
	~	metric ton [0,?]
	~		|

Cumulative Remanufacture= INTEG (
	tallying remanufacture,
		0)
	~	metric ton [0,?]
	~		|

Cumulative Reuse= INTEG (
	tallying reuse,
		0)
	~	metric ton [0,?]
	~		|

tallying recycle=
	aggregating recycled materials
	~	metric ton/year [0,?]
	~		|

tallying remanufacture=
	aggregating remanufactured products
	~	metric ton/year [0,?]
	~		|

allowable turbine component remanufacture=
	0.9
	~	Dmnl [0,1,0.05]
	~	Upper limit on the fraction of turbines in the plant before EOL turbine \
		components must be dealt with another way (not reusing). Represents the \
		technical limitations of remanufacturing and prevents infinite \
		remanufacturing.
	|

allowable turbine component reuse=
	0.2
	~	Dmnl [0,1,0.05]
	~	Upper limit on the fraction of turbine components in the plant that can be \
		reused before the components must be dealt with another way at EOL. \
		Represents technical limitations of reuse and the max lifetime that \
		turbine components offer.
	|

miles from extraction to production facility=
	200
	~	mile [0,500,1]
	~		|

********************************************************
	.cost models
********************************************************~
	|

annual change in cost of extraction and production=
	-0.357
	~	USD/metric ton/year
	~		|

cost of extraction and production=
	initial cost of extraction and production + annual change in cost of extraction and production\
		 * (Time - 1980)
	~	USD/metric ton [0,?]
	~		|

remanufacture process learning=
	remanufacture learning rate * TREND(Cumulative Remanufacture, 0.5 , 0 ) * MAX(0, Remanufacture Process Cost\
		) / one Year
	~	USD/metric ton/year
	~		|

recycle learning rate=
	-0.05
	~	Dmnl
	~		|

initial cost of extraction and production=
	65
	~	USD/metric ton [0,?,1]
	~		|

recycle process learning=
	recycle learning rate * TREND(Cumulative Recycle, 0.5 , 0 ) * MAX(0, Recycle Process Cost\
		) / one Year
	~	USD/metric ton/year
	~		|

remanufacture learning rate=
	-0.005
	~	Dmnl
	~		|

reuse process learning=
	-reuse learning rate * TREND(Cumulative Reuse, 0.5 , 0 ) * MAX(0, Reuse Process Cost\
		) / one Year
	~	USD/metric ton/year
	~		|

reuse learning rate=
	-0.001
	~	Dmnl
	~		|

Remanufacture Process Cost= INTEG (
	remanufacture process learning,
		initial cost of remanufacturing process)
	~	USD/metric ton [0,?]
	~		|

remanufactured material cost=
	miles from end use location to remanufacturing facility * cost of transportation
	+ Remanufacture Process Cost
	+  fraction reused product remanufactured * ( miles from reuse facility to remanufacturing facility\
		 * cost of transportation + Remanufacture Process Cost )
	+  miles from remanufacturing facility to product distribution facility * cost of transportation
	- remanufactured material strategic value
	~	USD/metric ton [0,?]
	~		|

Recycle Process Cost= INTEG (
	recycle process learning,
		initial cost of recycling process)
	~	USD/metric ton [0,?]
	~		|

recycled material cost=
	miles from end use location to recycling facility * cost of transportation
	+ Recycle Process Cost
	+ fraction remanufactured product to recycle * ( miles from remanufacturing facility to recycling facility\
		 * cost of transportation + Recycle Process Cost )
	+ miles from recycling to distribution facility * cost of transportation
	- recycled material strategic value
	~	USD/metric ton [0,?]
	~		|

linear material cost=
	cost of extraction and production
	+ miles from extraction to production facility * cost of transportation
	+ miles from end use location to landfill * cost of transportation
	+ miles from remanufacturing facility to landfill * cost of transportation
	+ miles from recycling facility to landfill * cost of transportation
	+ cost of landfilling
	~	USD/metric ton [0,?]
	~		|

reused material cost=
	miles from end use location to reuse facility * cost of transportation
	+ Reuse Process Cost
	+ miles from reuse facility to product distribution facility * cost of transportation
	- reused material strategic value
	~	USD/metric ton [0,?]
	~		|

Reuse Process Cost= INTEG (
	reuse process learning,
		initial cost of reuse process)
	~	USD/metric ton [0,?]
	~		|

initial cost of recycling process=
	200
	~	USD/metric ton [0,?]
	~		|

initial cost of remanufacturing process=
	250
	~	USD/metric ton [0,?,0.01]
	~		|

initial cost of reuse process=
	125
	~	USD/metric ton [0,?]
	~		|

cost of transportation= WITH LOOKUP (
	Time,
		([(1980,0)-(2050,0.5)],(1980,0.5),(2020,0.5),(2050,0.5) ))
	~	USD/metric ton/mile [0,?]
	~	NREL technical report, "Analysis of Transportation and Logistics \
		Challenges Affecting the Deployment of Larger Wind Turbines: Summary of \
		Results"
	|

cost of landfilling= WITH LOOKUP (
	Time,
		([(1980,60)-(2050,100)],(1980,95),(2050,95) ))
	~	USD/metric ton [0,?]
	~	$70 - $120 USD/metric ton is the range from source, "Construction and Demolition \
		Waste Characterization and Market Analysis Report", prepared for CT Dept \
		of Energy and Env Protection by Green Seal Environmental, Inc.
		Value used is midpoint of the above range.
	|

recycled material strategic value=
	0
	~	USD/metric ton [0,100]
	~	value of using recycled material  - reduces overall cost of recycled \
		material
	|

remanufactured material strategic value=
	0
	~	USD/metric ton [0,100]
	~	value of using remanufactured material - reduces overall remanufactured \
		material cost
	|

reused material strategic value=
	0
	~	USD/metric ton [0,100]
	~	negative cost representing the value of using reused material instead of \
		virgin
	|

miles from end use location to recycling facility=
	100
	~	mile [0,500,1]
	~		|

miles from end use location to remanufacturing facility=
	100
	~	mile [0,500,1]
	~		|

miles from end use location to reuse facility=
	100
	~	mile [0,500,1]
	~		|

miles from reuse facility to product distribution facility=
	100
	~	mile [0,500,1]
	~		|

miles from reuse facility to remanufacturing facility=
	0
	~	mile [0,500,1]
	~		|

miles from recycling facility to landfill=
	100
	~	mile [0,500,1]
	~		|

miles from remanufacturing facility to landfill=
	100
	~	mile [0,500,1]
	~		|

miles from end use location to landfill=
	200
	~	mile [0,500,1]
	~		|

miles from recycling to distribution facility=
	100
	~	mile [0,500,1]
	~		|

miles from remanufacturing facility to recycling facility=
	0
	~	mile [0,500,1]
	~		|

miles from remanufacturing facility to product distribution facility=
	100
	~	mile [0,500,1]
	~		|

