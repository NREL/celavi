{UTF-8}
check fraction sum[component]=
	Fraction Reuse[component] + Fraction Remanufacture[component] + Fraction Recycle[component\
		]
	~	Dmnl [0,1]
	~		|

reuse favorability over linear[component]=
	reused material cost[component] / linear material cost[component]
	~	Dmnl [0,?]
	~		|

Fraction Reuse[component]= INTEG (
	changing fraction reuse[component],
		fraction used product reused initial value[component])
	~	Dmnl [0,1]
	~		|

remanufacture favorability[component]=
	IF THEN ELSE(lowest cost pathway[component] = remanufacture favorability over linear\
		[component],
	             remanufacture favorability over linear[component],
	             10)
	~	Dmnl [0,?]
	~		|

rate of increasing recycle fraction[component]= WITH LOOKUP (
	recycle favorability[component],
		([(0,0)-(10,0.01)],(0,0.01),(0.275229,0.00964912),(0.422018,0.00995614),(1,0.0090825\
		),(1.40673,0.00929825),(2,0),(10,0
		) ))
	~	
	~	\!recycle favorability
	|

recycle favorability[component]=
	IF THEN ELSE(lowest cost pathway[component] = recycle favorability over linear[component\
		],
	             recycle favorability over linear[component],
	             10)
	~	Dmnl [0,?]
	~		|

rate of increasing reuse fraction[component]= WITH LOOKUP (
	reuse favorability[component],
		([(0,0)-(10,0.01)],(0,0.01),(0.275229,0.00903509),(0.519878,0.00833333),(0.99,0.00758772\
		),(1,0),(2,0),(10,0) ))
	~	
	~	\!reuse favorability\!rate of increasing fraction reuse
	|

increasing fraction recycle[component]=
	IF THEN ELSE(check fraction sum[component] < 1 - rate of increasing recycle fraction\
		[component] :AND:
	             Fraction Recycle[component] < 1 - rate of increasing recycle fraction[component\
		],
	             increase recycle[component] * rate of increasing recycle fraction[component\
		],
	             0)
	~	Dmnl/year
	~		|

rate of increasing remanufacture fraction[component]= WITH LOOKUP (
	remanufacture favorability[component],
		([(0,0)-(10,0.008)],(0,0.00736842),(0.165138,0.00526316),(0.360856,0.00342105),(0.550459\
		,0.00184211),(1,0),(2,0),(10,0
		) ))
	~	
	~	\!remanufacture favorability
	|

reuse favorability[component]=
	IF THEN ELSE(lowest cost pathway[component] = reuse favorability over linear[component\
		],
	             reuse favorability over linear[component],
	             10)
	~	Dmnl [0,?]
	~		|

Cumulative Recycle[component]= INTEG (
	tallying recycle[component],
		0)
	~	each [0,?]
	~		|

Cumulative Remanufacture[component]= INTEG (
	tallying remanufacture[component],
		0)
	~	each [0,?]
	~		|

recycle process learning[component]=
	-0.05 * TREND(Cumulative Recycle[component], 0.5 , 0 ) * MAX(0, Recycle Process Cost\
		[component])
	~	USD/each
	~		|

allowable turbine component remanufacture[component]=
	0.9
	~	Dmnl [0,1,0.05]
	~	Upper limit on the fraction of turbines in the plant before EOL turbine \
		components must be dealt with another way (not reusing). Represents the \
		technical limitations of remanufacturing and prevents infinite \
		remanufacturing.
	|

allowable turbine component reuse[component]=
	0.2
	~	Dmnl [0,1,0.05]
	~	Upper limit on the fraction of turbine components in the plant that can be \
		reused before the components must be dealt with another way at EOL. \
		Represents technical limitations of reuse and the max lifetime that \
		turbine components offer.
	|

remanufacture process learning[component]=
	-0.05 * TREND(Cumulative Remanufacture[component], 0.5 , 0 ) * MAX(0, Remanufacture Process Cost\
		[component])
	~	USD/each
	~		|

tallying remanufacture[component]=
	aggregating remanufactured products[component]
	~	each/year [0,?]
	~		|

tallying reuse[component]=
	aggregating reused products[component]
	~	each/year [0,?]
	~		|

remaining remanufacture[component]=
	allowable turbine component remanufacture[component] * number of turbines - Cumulative Remanufacture\
		[component]
	~	each [0,?]
	~	calculates the number of turbine components that can still be \
		remanufactured
	|

remaining reuse[component]=
	allowable turbine component reuse[component] * number of turbines - Cumulative Reuse\
		[component] - Fraction Reuse[component] * number of turbines
	~	each [0,?]
	~	calculates the number of turbine components that can still be reused
	|

Cumulative Reuse[component]= INTEG (
	tallying reuse[component],
		0)
	~	each [0,?]
	~		|

reuse process learning[component]=
	-0.05 * TREND(Cumulative Reuse[component], 0.5 , 0 ) * MAX(0, Reuse Process Cost[component\
		])
	~	USD/each
	~		|

tallying recycle[component]=
	aggregating recycled materials[component]
	~	each/year [0,?]
	~		|

Recycle Process Cost[component]= INTEG (
	recycle process learning[component],
		initial cost of recycling process[component])
	~	USD/each [0,?]
	~		|

recycled material cost[component]=
	( ( miles from end use location to recycling facility[component] * cost of transportation\
		[component] * component mass[component]
	              + Recycle Process Cost[component] )
	  + fraction remanufactured product to recycle[component]
	    * ( miles from remanufacturing facility to recycling facility[component] * cost of transportation\
		[component] * component mass[component] 
	        + Recycle Process Cost[component] )
	  + ( miles from recycling to distribution facility[component] * cost of transportation\
		[component] * component mass[component] ) ) 
	- recycled material strategic value[component]
	~	USD/each [0,?]
	~		|

remanufactured material cost[component]=
	( ( miles from end use location to remanufacturing facility[component] * cost of transportation\
		[component] * component mass
	[component] + Remanufacture Process Cost[component] )
	  +  fraction reused product remanufactured[component] * ( miles from reuse facility to remanufacturing facility\
		[component
	] * cost of transportation[component]  * component mass[component]
	        + Remanufacture Process Cost[component] )
	  +  ( miles from remanufacturing facility to product distribution facility[component\
		] * cost of transportation[component
	] * component mass[component] ) ) 
	- remanufactured material strategic value[component]
	~	USD/each
	~		|

initial cost of recycling process[blade]=
	75 ~~|
initial cost of recycling process[nacelle]=
	50 ~~|
initial cost of recycling process[tower]=
	75 ~~|
initial cost of recycling process[foundation]=
	80 ~~|
initial cost of recycling process[road]=
	150 ~~|
initial cost of recycling process[electrical]=
	95
	~	USD/each [0,?]
	~		|

initial cost of remanufacturing process[blade]=
	100 ~~|
initial cost of remanufacturing process[nacelle]=
	100 ~~|
initial cost of remanufacturing process[tower]=
	100 ~~|
initial cost of remanufacturing process[foundation]=
	75 ~~|
initial cost of remanufacturing process[road]=
	130 ~~|
initial cost of remanufacturing process[electrical]=
	90
	~	USD/each [0,?,0.01]
	~		|

Remanufacture Process Cost[component]= INTEG (
	remanufacture process learning[component],
		initial cost of remanufacturing process[component])
	~	USD/each
	~		|

Reuse Process Cost[component]= INTEG (
	reuse process learning[component],
		initial cost of reuse process[component])
	~	USD/each [0,?]
	~		|

increasing fraction remanufacture[component]=
	IF THEN ELSE(check fraction sum[component] < 1 - rate of increasing remanufacture fraction\
		[component] :AND:
	             Fraction Remanufacture[component] < 1 - rate of increasing remanufacture fraction\
		[component],
	             increase remanufacture[component] * rate of increasing remanufacture fraction\
		[component],
	             0)
	~	Dmnl/year
	~		|

initial cost of reuse process[blade]=
	1000 ~~|
initial cost of reuse process[nacelle]=
	35 ~~|
initial cost of reuse process[tower]=
	55 ~~|
initial cost of reuse process[foundation]=
	40 ~~|
initial cost of reuse process[road]=
	40 ~~|
initial cost of reuse process[electrical]=
	75
	~	USD/each [0,?]
	~		|

reused material cost[component]=
	miles from end use location to reuse facility[component] * cost of transportation[component\
		] * component mass[component
	]
	+ Reuse Process Cost[component]
	+ miles from reuse facility to product distribution facility[component] * cost of transportation\
		[component] * component mass
	[component]
	- reused material strategic value[component]
	~	USD/each [0,?]
	~		|

cost of transportation[component]= WITH LOOKUP (
	Time,
		([(0,0.4)-(100,0.5)],(0,0.5),(100,0.5) ))
	~	USD/metric ton/mile
	~	NREL technical report, "Analysis of Transportation and Logistics \
		Challenges Affecting the Deployment of Larger Wind Turbines: Summary of \
		Results"
	|

increase recycle[component]=
	IF THEN ELSE(recycle favorability over linear[component] < 1,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

increase remanufacture[component]=
	IF THEN ELSE(remanufacture favorability over linear[component] < 1,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

increase reuse[component]=
	IF THEN ELSE(reuse favorability over linear[component] < 1,
	             1,
	             0)
	~	Dmnl [0,1]
	~		|

cost of landfilling[component]= WITH LOOKUP (
	Time,
		([(0,0)-(100,100)],(0,70),(100,70) ))
	~	USD/metric ton [0,?]
	~	$70 - $120 USD/metric ton is the range from source, "Construction and Demolition \
		Waste Characterization and Market Analysis Report", prepared for CT Dept \
		of Energy and Env Protection by Green Seal Environmental, Inc.
		Value used is midpoint of the above range.
	|

cost of extraction and production[blade]= WITH LOOKUP (
	Time,
		([(0,0)-(100,40)],(0,20),(100,25) )) ~~|
cost of extraction and production[nacelle]= WITH LOOKUP (
	Time,
		([(0,0)-(100,40)],(0,40),(100,50) )) ~~|
cost of extraction and production[tower]= WITH LOOKUP (
	Time,
		([(0,0)-(100,75)],(0,40),(100,65) )) ~~|
cost of extraction and production[foundation]= WITH LOOKUP (
	Time,
		([(0,0)-(100,890)],(0,70),(100,87.5) )) ~~|
cost of extraction and production[road]= WITH LOOKUP (
	Time,
		([(0,0)-(100,160)],(0,125),(100,156.25) )) ~~|
cost of extraction and production[electrical]= WITH LOOKUP (
	Time,
		([(0,0)-(100,100)],(0,80),(100,100) ))
	~	USD/each [0,?]
	~		|

fraction used product reused initial value[component]=
	0
	~	Dmnl [0,1,0.01]
	~		|

Fraction Recycle[component]= INTEG (
	increasing fraction recycle[component],
		fraction used product recycled initial value[component])
	~	Dmnl [0,1]
	~		|

Fraction Remanufacture[component]= INTEG (
	increasing fraction remanufacture[component],
		fraction used product remanufactured initial value[component])
	~	Dmnl [0,1]
	~		|

reusing[component]=
	INTEGER( reaching end of life[component] * Fraction Reuse[component] )
	~	each/year [0,?]
	~		|

fraction used product disposed[component]=
	1 - Fraction Reuse[component] - fraction used product reused in other sectors[component\
		] - Fraction Remanufacture[component] - Fraction Recycle[component]
	~	Dmnl [0,1]
	~	default value 0.67
	|

fraction used product recycled initial value[component]=
	0
	~	Dmnl [0,1,0.01]
	~		|

fraction used product remanufactured initial value[component]=
	0
	~	Dmnl [0,1,0.01]
	~		|

remanufacturing[component]=
	reaching end of life[component] * Fraction Remanufacture[component]
	~	each/year [0,?]
	~		|

recycling[component]=
	INTEGER(reaching end of life[component] * Fraction Recycle[component])
	~	each/year [0,?]
	~		|

linear material cost[component]=
	cost of extraction and production[component] + miles from extraction to production facility\
		[component] * cost of transportation[component] * component mass[component]
	+ 
	( ( miles from end use location to landfill[component] 
	                              * cost of transportation[component]
	                              * component mass[component]  + 
	cost of landfilling[component] * component mass[component] )
	  + 
	     ( miles from remanufacturing facility to landfill[component] * cost of transportation\
		[component] * component mass[component] + cost of landfilling[component] * component mass\
		[component] )
	  + ( miles from recycling facility to landfill[component] * cost of transportation[\
		component] * component mass[component]  + cost of landfilling[component] * component mass\
		[component]) )
	~	USD/each [0,?]
	~		|

annual demand[blade]=
	(3 * number of turbines * TIME STEP / turbine assembly time) / one Year
	* PULSE TRAIN(initial construction time, turbine assembly time , component lifetime[\
		blade] + supply chain delay[blade], 1000 ) ~~|
annual demand[nacelle]=
	( number of turbines * TIME STEP / turbine assembly time ) / one Year
	* PULSE TRAIN(initial construction time, turbine assembly time ,
	              component lifetime[nacelle] + supply chain delay[nacelle] , 1000 ) ~~|
annual demand[tower]=
	( number of turbines * TIME STEP / turbine assembly time ) / one Year
	* PULSE TRAIN(initial construction time, turbine assembly time ,
	              component lifetime[tower] + supply chain delay[tower], 1000 ) ~~|
annual demand[foundation]=
	( number of turbines * TIME STEP / initial construction time ) / one Year
	* PULSE TRAIN(0, initial construction time ,
	              component lifetime[foundation] + supply chain delay[foundation] , 1000\
		 ) ~~|
annual demand[road]=
	TIME STEP/one Year * PULSE TRAIN(0, initial construction time,
	                                 component lifetime[road] + supply chain delay[road]\
		 , 1000 ) * one Unit[road] / one Year ~~|
annual demand[electrical]=
	TIME STEP/one Year * PULSE TRAIN(0, initial construction time, component lifetime[electrical\
		] + supply chain delay[electrical] , 1000 ) * one Unit[electrical
	] / one Year
	~	each/year [0,?,10000]
	~	The values are made up. The shape of the curve covers some initial BOS \
		construction of the wind farm, followed by full turbine installation, \
		followed by regular O&M over the lifetime of the farm, followed by 2 more \
		rounds of turbine replacement and continued O&M.
	|

component mass[blade]=
	0.5 ~~|
component mass[nacelle]=
	0.75 ~~|
component mass[tower]=
	5 ~~|
component mass[foundation]=
	1 ~~|
component mass[road]=
	3 ~~|
component mass[electrical]=
	5
	~	metric ton/each [0,?]
	~		|

remanufacture favorability over linear[component]=
	remanufactured material cost[component] / linear material cost[component]
	~	Dmnl [0,?]
	~		|

recycle favorability over linear[component]=
	recycled material cost[component] / linear material cost[component]
	~	Dmnl [0,?]
	~		|

one Year=
	1
	~	year [1,1]
	~		|

one Unit[component]=
	1
	~	each [1,1]
	~		|

aggregating recycled materials[component]=
	recycling[component] + recycling failed remanufactured[component] - landfilling nonrecyclables\
		[component]
	~	each/year [0,?]
	~		|

aggregating remanufactured products[component]=
	remanufacturing[component] + remanufacturing nonreusables[component] - landfilling failed remanufactured\
		[component] - recycling failed remanufactured[component]
	~	each/year [0,?]
	~		|

aggregating reused products[component]=
	reusing[component] - remanufacturing nonreusables[component]
	~	each/year [0,?]
	~		|

annual virgin material demand[component]=
	annual demand[component]
	- aggregating recycled materials[component]
	- aggregating reused products[component]
	- aggregating remanufactured products[component]
	~	each/year [0,?]
	~		|

recycling failed remanufactured[component]=
	(remanufacturing[component] + remanufacturing nonreusables[component]) * fraction remanufactured product to recycle\
		[component]
	~	each/year [0,?]
	~	components that could not be put back into a using product
	|

relative landfill[component]=
	( reaching end of life[component] - (landfilling[component] + landfilling failed remanufactured\
		[component] + landfilling nonrecyclables[component]) ) / ( reaching end of life[component\
		] + 0.001)
	~	Dmnl
	~		|

Products in Use[component]= INTEG (
	shipping[component]-reaching end of life[component],
		initial components in use[component])
	~	each [0,?]
	~		|

fraction used product reused in other sectors[component]=
	0
	~	Dmnl [0,1,0.01]
	~		|

cumulative landfill fraction[component]=
	Landfill and Incineration[component] / (Total Extraction[component] + 0.001)
	~	Dmnl [0,?]
	~		|

disposing[component]=
	reaching end of life[component] * fraction used product disposed[component]
	~	each/year [0,?]
	~		|

distributing[component]=
	extracting[component] + aggregating recycled materials[component]
	~	each/year [0,?]
	~		|

environmental impact from landfill transportation[component]=
	environmental impact of transportation[component] * landfilling[component] * miles from end use location to landfill\
		[component]
	~	impact/year
	~		|

environmental impact from recycling transportation[component]=
	environmental impact of transportation[component] *
	( recycling[component] * miles from end use location to recycling facility[component\
		]
	  + recycling failed remanufactured[component] * miles from remanufacturing facility to recycling facility\
		[component]
	  + landfilling nonrecyclables[component] * miles from recycling facility to landfill\
		[component]
	  + aggregating recycled materials[component] * miles from recycling to distribution facility\
		[component] )
	~	impact/year
	~		|

environmental impact from remanufacturing transportation[component]=
	environmental impact of transportation[component] *
	( remanufacturing[component] * miles from end use location to remanufacturing facility\
		[component] 
	  + remanufacturing nonreusables[component] * miles from reuse facility to remanufacturing facility\
		[component]
	  + aggregating remanufactured products[component] * miles from remanufacturing facility to product distribution facility\
		[component] 
	  + landfilling failed remanufactured[component] * miles from remanufacturing facility to landfill\
		[component] )
	~	impact/year
	~		|

environmental impact from reuse transportation[component]=
	environmental impact of transportation[component] * 
	( reusing[component] * miles from end use location to reuse facility[component]
	  + aggregating reused products[component] * miles from reuse facility to product distribution facility\
		[component] )
	~	impact/year
	~	total mileage * environmental impact per kgmile
	|

Products at End of Life[component]= INTEG (
	reaching end of life[component]-disposing[component]-recycling[component]-remanufacturing\
		[component]-reusing[component]-reusing in other sectors[component],
		0)
	~	each [-1,?]
	~		|

landfilling nonrecyclables[component]=
	(recycling[component] + recycling failed remanufactured[component]) * fraction of recycling to landfill\
		[component]
	~	each/year [0,?]
	~		|

Products Sent to Other Sectors[component]= INTEG (
	reusing in other sectors[component],
		0)
	~	each [0,?]
	~		|

Raw Material Extraction[component]= INTEG (
	-extracting[component],
		1e+10)
	~	each [0,?]
	~		|

reaching end of life[component]= DELAY MATERIAL (
	shipping[component], component lifetime[component] , 0 , 0)
	~	each/year [0,?]
	~	* used to be a DELAY CONVEYOR( shipping ,
		                product lifetime , 
		                0 ,
		                initprofile , 
		                initial products at end of life , 
		                product lifetime )
		* first argument to DELAY CONVEYOR was originally transporting * IF THEN \
		ELSE(Products in Use <= 0, 0, 1)
	|

environmental impact per kgyear[component]=
	( aggregating reused products[component] * environmental impact of reusing process[component\
		]
	  + aggregating remanufactured products[component] * environmental impact of remanufacturing process\
		[component]
	  + aggregating recycled materials[component] * environmental impact of recycling process\
		[component]
	  + extracting[component] * environmental impact of extracting process[component] 
	  + environmental impact from landfill transportation[component]
	  + environmental impact from recycling transportation[component]
	  + environmental impact from remanufacturing transportation[component]
	  + environmental impact from reuse transportation[component] ) / (Products in Use[component\
		] + 0.001)
	~	impact/(each*year) [0,?]
	~		|

extracting[component]=
	annual virgin material demand[component]
	~	each/year [0,?]
	~		|

landfilling[component]=
	disposing[component]
	~	each/year [0,?]
	~		|

Production[component]= INTEG (
	aggregating remanufactured products[component] + distributing[component] - producing\
		[component],
		100)
	~	each [0,?]
	~		|

Material Distribution[component]= INTEG (
	aggregating recycled materials[component]+extracting[component]-distributing[component\
		],
		100)
	~	each [0,?]
	~		|

remanufacturing nonreusables[component]=
	reusing[component] * fraction reused product remanufactured[component]
	~	each/year [0,?]
	~		|

initial construction time=
	2
	~	year [0,10,0.25]
	~		|

Landfill and Incineration[component]= INTEG (
	landfilling[component]+landfilling failed remanufactured[component]+landfilling nonrecyclables\
		[component],
		0)
	~	each [0,?]
	~		|

Landfill Transport[component]= INTEG (
	disposing[component]-landfilling[component],
		0)
	~	each [0,?]
	~		|

Product Distribution[component]= INTEG (
	aggregating reused products[component]
	+ producing[component]
	- shipping[component],
		100)
	~	each [0,?]
	~		|

Product Remanufacture[component]= INTEG (
	remanufacturing[component]+remanufacturing nonreusables[component]-aggregating remanufactured products\
		[component]-landfilling failed remanufactured[component]-recycling failed remanufactured\
		[component],
		0)
	~	each [0,?]
	~		|

landfilling failed remanufactured[component]=
	(remanufacturing[component] + remanufacturing nonreusables[component]) * fraction of remanufacture to landfill\
		[component]
	~	each/year [0,?]
	~		|

shipping[component]= DELAY MATERIAL (
	producing[component] + aggregating reused products[component], 
	supply chain delay[component],
	0,
	0)
	~	each/year [0,?]
	~		|

tallying extraction[component]=
	extracting[component]
	~	each/year [0,?]
	~		|

Total Extraction[component]= INTEG (
	tallying extraction[component],
		0)
	~	each [0,?]
	~		|

Material Recycle[component]= INTEG (
	recycling[component]+recycling failed remanufactured[component]-aggregating recycled materials\
		[component]-landfilling nonrecyclables[component],
		0)
	~	each [-0.1,?]
	~	aka recycling
	|

number of turbines=
	200
	~	each [0,?,1]
	~		|

producing[component]=
	distributing[component] + aggregating remanufactured products[component]
	~	each/year [0,?]
	~		|

Product Reuse[component]= INTEG (
	reusing[component]-aggregating reused products[component]-remanufacturing nonreusables\
		[component],
		0)
	~	each [0,?]
	~		|

reusing in other sectors[component]=
	reaching end of life[component] * fraction used product reused in other sectors[component\
		]
	~	each/year [0,?]
	~		|

turbine assembly time=
	1
	~	year [0,5,0.25]
	~		|

environmental impact of extracting process[component]=
	15
	~	impact/each [0,?]
	~		|

environmental impact of recycling process[component]=
	11
	~	impact/each [0,?]
	~		|

environmental impact of remanufacturing process[component]=
	8
